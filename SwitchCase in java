package intro;


import java.util.Scanner;

public class switchcase {
    static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String fruit=sc.nextLine();
        switch (fruit){
            case "Mango":
                System.out.println("It is sweet fruit");
                break;
            case "Apple":
                System.out.println("it is from kashmir");
                break;
            case "Grapes":
                System.out.println("It is a sour");
                break;
            default:
                System.out.println("Choose a valid fruit");
        }
    }
}
